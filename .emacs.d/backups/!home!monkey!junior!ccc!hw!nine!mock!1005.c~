#include <stdio.h>

#define N 1000

int main()
{
	int t,n,i,j;
	int a[N];

	scanf("%d",&t);

	while(t--){
		scanf("%d",&n);
		for(i = 1; i <= n; i++){
			a[i] = i;
		}
		for(i = 1; i < n; i += 2){
			printf("%d ",a[i]);
			if(i == n - 1){
				printf("%d ",a[n]);
				break;
			}
			a[++n] = a[i + 1];
		}
	}

	return 0;
}
