#inclue <stdio.h>

#define N 101

int main()
{
	int t,n,i,j,k,c,temp;
	int a[N];

	scanf("%d",&t);
	while(t--){
		scanf("%d",&n);
		for(i = 0; i < n; i++){
			scanf("%d",&a[i]);
		}
		c = 0;
		for(i = 0; i < n - 1; i++){
			k = i;
			for(j = i + 1; j < n; j++){
				if(a[j] < a[k]){
					k = j;
					c++;
				}
			}
			if(k != i){
				temp = a[i];
				a[i] = a[k];
				a[k] = temp;
			}
		}
		printf("%d\n",c);
	}

	return 0;
}	
	
